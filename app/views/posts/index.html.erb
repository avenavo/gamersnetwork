<% @header = 'Posts' %>

  <div class="col-md-11 centered" , style="margin-top:7%;">


    <div class="row">
      <div class="col-md-3">
     <%= image_tag(current_user.asset.url(:medium), class: 'img-circle img-medium border')  %></div>
      <div class="col-md-9">   <%= render 'form' %>
      </div>
    </div>

  <hr>
   <div id='posts'>
    <%= render @posts %>
   </div>
    <%= will_paginate @posts, renderer: BootstrapPagination::Rails  %>

</div>




